testing
testForSigHash

	| publicKey sig max start testHash int beginSeconds endSeconds |
	publicKey := DSAPublicKey
		p: 
		q: 950007512582893210309817974627632677445789610567
		g: 
		y: .
	sig := Array
		with: 937700723394128569640526739705823827102731619578
		with: 252508435170647782870798129364071506472233742236.
	max := 1461501637330902918203684832716283019655932542975.
	start := 6151.
	testHash := start asByteArrayOfSize: 20.
	beginSeconds := Time now asSeconds.
	[testHash asInteger > max
		or: [publicKey
				verifySignature: sig
				ofMessageHash: (SHA1 hashMessage: testHash)]]
		whileFalse: [int := testHash asInteger + 1.
			testHash := int asByteArrayOfSize: 20.
			int \\ 100 = 0
				ifTrue: [
					endSeconds := Time now asSeconds.
					Transcript 
						cr; 
						nextPutAll: testHash printString;
						nextPutAll: ' in ';
						nextPutAll: (endSeconds - beginSeconds) asString;
						nextPutAll: ' secs.  ';
						nextPutAll: ((max - int // 100) * (endSeconds - beginSeconds) // 3600) asString;
						nextPutAll: ' hrs remain';
						flush.
					beginSeconds := Time now asSeconds]].
	^ testHash